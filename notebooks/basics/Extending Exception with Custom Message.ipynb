   "source": [
    "class AgeLimitError(Exception):\n",
    "    def __init__(self, age):\n",
    "        super().__init__(f\"Age {age} is below allowed limit\")\n",
    "\n",
    "def validate_age(age):\n",
    "    if age < 18:\n",
    "        raise AgeLimitError(age)\n",
    "\n",
    "validate_age(15)"
