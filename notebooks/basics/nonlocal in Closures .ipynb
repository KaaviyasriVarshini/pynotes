    "def power(base):\n",
    "    exponent = 2\n",
    "\n",
    "    def calculate():\n",
    "        nonlocal exponent\n",
    "        exponent += 1\n",
    "        return base ** exponent\n",
    "\n",
    "    return calculate\n",
    "\n",
    "p = power(2)\n",
    "print(p())\n",
    "print(p())"
