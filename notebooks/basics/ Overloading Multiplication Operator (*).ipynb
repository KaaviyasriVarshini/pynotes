    "class Repeater:\n",
    "    def __init__(self, text):\n",
    "        self.text = text\n",
    "\n",
    "    def __mul__(self, times):\n",
    "        return self.text * times\n",
    "\n",
    "r = Repeater(\"AI \")\n",
    "print(r * 3)  # Output: AI AI AI "
