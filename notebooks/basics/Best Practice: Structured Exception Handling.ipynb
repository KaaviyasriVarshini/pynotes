    "def divide(a, b):\n",
    "    try:\n",
    "        return a / b\n",
    "    except ZeroDivisionError:\n",
    "        return \"Division by zero error\"\n",
    "    except TypeError:\n",
    "        return \"Invalid input type\"\n",
    "\n",
    "print(divide(10, 0))"
