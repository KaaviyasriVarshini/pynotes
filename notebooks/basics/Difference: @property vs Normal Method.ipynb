    "class User:\n",
    "    def __init__(self, name):\n",
    "        self._name = name\n",
    "\n",
    "    def get_name(self):\n",
    "        return self._name\n",
    "\n",
    "    @property\n",
    "    def name(self):\n",
    "        return self._name\n",
    "\n",
    "u = User(\"Bob\")\n",
    "print(u.get_name())  # Method call\n",
    "print(u.name)        # Property access"
   ]
