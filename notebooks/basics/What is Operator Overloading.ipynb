    "class Number:\n",
    "    def __init__(self, value):\n",
    "        self.value = value\n",
    "\n",
    "    def __add__(self, other):\n",
    "        return self.value + other.value\n",
    "\n",
    "n1 = Number(10)\n",
    "n2 = Number(20)\n",
    "\n",
    "print(n1 + n2)  # Output: 30"
