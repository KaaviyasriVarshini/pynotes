   "source": [
    "class Account:\n",
    "    def __init__(self, balance):\n",
    "        self._balance = balance\n",
    "\n",
    "    @property\n",
    "    def balance(self):\n",
    "        return self._balance\n",
    "\n",
    "    @balance.deleter\n",
    "    def balance(self):\n",
    "        del self._balance\n",
    "\n",
    "acc = Account(1000)\n",
    "del acc.balance"
   ]
