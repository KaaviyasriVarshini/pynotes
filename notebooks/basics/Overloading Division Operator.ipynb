   "source": [
    "class Calculator:\n",
    "    def __init__(self, value):\n",
    "        self.value = value\n",
    "\n",
    "    def __truediv__(self, other):\n",
    "        return self.value / other.value\n",
    "\n",
    "c1 = Calculator(100)\n",
    "c2 = Calculator(4)\n",
    "\n",
    "print(c1 / c2)  # Output: 25.0"
   ]
