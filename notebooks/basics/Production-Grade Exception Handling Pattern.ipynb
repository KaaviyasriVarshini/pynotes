    "        return a / b\n",
    "    except ZeroDivisionError as e:\n",
    "        return f\"Error: {e}\"\n",
    "    except Exception as e:\n",
    "        return f\"Unexpected error: {e}\"\n",
    "    finally:\n",
    "        print(\"Operation attempted\")\n",
