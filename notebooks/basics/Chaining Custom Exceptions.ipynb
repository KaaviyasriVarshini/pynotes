   "source": [
    "class InitialError(Exception):\n",
    "    pass\n",
    "\n",
    "class DerivedError(Exception):\n",
    "    pass\n",
    "\n",
    "try:\n",
    "    try:\n",
    "        raise InitialError(\"Initial failure\")\n",
    "    except InitialError as e:\n",
    "        raise DerivedError(\"Follow-up failure\") from e\n",
    "except DerivedError as final_error:\n",
    "    print(final_error)"
   ]
