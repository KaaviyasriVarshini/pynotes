    "def counter():\n",
    "    value = 0\n",
    "    def increment():\n",
    "        nonlocal value\n",
    "        value += 1\n",
    "        return value\n",
    "    return increment\n",
    "\n",
    "count = counter()\n",
    "print(count())\n",
    "print(count())"
