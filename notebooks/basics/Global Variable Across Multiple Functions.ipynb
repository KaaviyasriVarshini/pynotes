    "status = \"inactive\"\n",
    "\n",
    "def activate():\n",
    "    global status\n",
    "    status = \"active\"\n",
    "\n",
    "def show_status():\n",
    "    print(status)\n",
    "\n",
    "activate()\n",
    "show_status()  # Output: active"
