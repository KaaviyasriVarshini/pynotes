   "source": [
    "from datetime import datetime\n",
    "from zoneinfo import ZoneInfo\n",
    "\n",
    "timestamp = 1732452305\n",
    "\n",
    "utc = datetime.fromtimestamp(timestamp, ZoneInfo(\"UTC\"))\n",
    "local = utc.astimezone(ZoneInfo(\"Asia/Kolkata\"))\n",
    "\n",
    "print(\"UTC:\", utc)\n",
    "print(\"Local:\", local)"
   ]
