    "import re\n",
    "\n",
    "def validate_phone(phone):\n",
    "    pattern = r\"^\\+?\\d{10,13}$\"\n",
    "    return bool(re.match(pattern, phone))\n",
    "\n",
    "print(validate_phone(\"+911234567890\"))  # True\n",
    "print(validate_phone(\"12345\"))          # False"
   ]
