    "total = 100\n",
    "\n",
    "def calculate():\n",
    "    total = 50\n",
    "\n",
    "    def adjust():\n",
    "        nonlocal total\n",
    "        total += 10\n",
    "\n",
    "    adjust()\n",
    "    return total\n",
    "\n",
    "print(calculate())  # Output: 60\n",
    "print(total)        # Output: 100"
