    "import time\n",
    "\n",
    "last_request = time.time()\n",
    "\n",
    "def can_execute():\n",
    "    global last_request\n",
    "    current = time.time()\n",
    "    if current - last_request >= 5:\n",
    "        last_request = current\n",
    "        return True\n",
    "    return False\n",
    "\n",
    "print(can_execute())"
   ]
