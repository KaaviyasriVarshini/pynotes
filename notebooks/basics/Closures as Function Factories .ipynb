    "def logger(prefix):\n",
    "    def log(message):\n",
    "        print(f\"{prefix}: {message}\")\n",
    "    return log\n",
    "\n",
    "info_logger = logger(\"INFO\")\n",
    "error_logger = logger(\"ERROR\")\n",
    "\n",
    "info_logger(\"System started\")\n",
    "error_logger(\"System failed\")"
   ]
