    "class Counter:\n",
    "    def __init__(self):\n",
    "        self.value = 0\n",
    "\n",
    "    def increment(self):\n",
    "        self.value += 1\n",
    "\n",
    "counter = Counter()\n",
    "counter.increment()\n",
    "print(counter.value)  # Output: 1"
