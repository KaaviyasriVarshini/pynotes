    "class Counter:\n",
    "    def __init__(self, value):\n",
    "        self.value = value\n",
    "\n",
    "    def __iadd__(self, other):\n",
    "        self.value += other\n",
    "        return self\n",
    "\n",
    "c = Counter(10)\n",
    "c += 5\n",
    "print(c.value)  # Output: 15"
   ]
