    "class Logger:\n",
    "    def __init__(self, func):\n",
    "        self.func = func\n",
    "\n",
    "    def __call__(self, *args, **kwargs):\n",
    "        print(\"Calling function...\")\n",
    "        return self.func(*args, **kwargs)\n",
    "\n",
    "@Logger\n",
    "def show():\n",
    "    print(\"Processing data\")\n",
    "\n",
    "show()"
