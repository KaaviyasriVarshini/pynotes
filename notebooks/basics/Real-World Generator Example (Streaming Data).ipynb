    "def read_large_file(filename):\n",
    "    with open(filename, \"r\") as file:\n",
    "        for line in file:\n",
    "            yield line.strip()\n",
    "\n",
    "for line in read_large_file(\"data.csv\"):\n",
    "    print(line)"
