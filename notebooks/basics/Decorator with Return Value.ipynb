   "source": [
    "def uppercase(func):\n",
    "    def wrapper():\n",
    "        result = func()\n",
    "        return result.upper()\n",
    "    return wrapper\n",
    "\n",
    "@uppercase\n",
    "def message():\n",
    "    return \"hello world\"\n",
    "\n",
    "print(message())"
   ]
