    "def log_event(event_type, *args, **kwargs):\n",
    "    print(f\"Event: {event_type}\")\n",
    "    if args:\n",
    "        print(\"Details:\", args)\n",
    "    if kwargs:\n",
    "        print(\"Metadata:\", kwargs)\n",
    "\n",
    "log_event(\"LOGIN\", \"UserID:101\", ip=\"192.168.1.1\", status=\"success\")"
   ]
