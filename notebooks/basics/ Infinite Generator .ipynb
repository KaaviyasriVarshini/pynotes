    "def infinite_counter():\n",
    "    num = 1\n",
    "    while True:\n",
    "        yield num\n",
    "        num += 1\n",
    "\n",
    "counter = infinite_counter()\n",
    "print(next(counter))\n",
    "print(next(counter))"
