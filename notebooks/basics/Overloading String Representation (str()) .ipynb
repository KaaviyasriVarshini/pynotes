   "source": [
    "class Product:\n",
    "    def __init__(self, name, price):\n",
    "        self.name = name\n",
    "        self.price = price\n",
    "\n",
    "    def __str__(self):\n",
    "        return f\"{self.name} costs ${self.price}\"\n",
    "\n",
    "p = Product(\"Laptop\", 1200)\n",
    "print(p)  # Laptop costs $1200"
   ]
