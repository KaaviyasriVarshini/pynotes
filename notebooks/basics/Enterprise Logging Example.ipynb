    "from datetime import datetime\n",
    "\n",
    "def log_event(event):\n",
    "    timestamp = datetime.now().strftime(\"%Y-%m-%d %H:%M:%S\")\n",
    "    print(f\"[{timestamp}] {event}\")\n",
    "\n",
    "log_event(\"Server Started\")\n",
    "log_event(\"User Login Successful\")"
   ]
