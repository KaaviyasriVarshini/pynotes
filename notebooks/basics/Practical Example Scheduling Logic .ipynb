    "from datetime import datetime, timedelta\n",
    "\n",
    "meeting_time = datetime.now() + timedelta(days=2)\n",
    "\n",
    "if datetime.now() < meeting_time:\n",
    "    print(\"Meeting scheduled\")\n",
    "else:\n",
    "    print(\"Meeting overdue\")"
