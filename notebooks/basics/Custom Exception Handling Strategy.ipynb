    "class InvalidAgeError(Exception):\n",
    "    pass\n",
    "\n",
    "def validate_age(age):\n",
    "    if age < 18:\n",
    "        raise InvalidAgeError(\"Age must be 18 or above\")\n",
    "\n",
    "try:\n",
    "    validate_age(16)\n",
    "except InvalidAgeError as e:\n",
    "    print(e)"
