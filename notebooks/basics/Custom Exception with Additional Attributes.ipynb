   "source": [
    "class TransactionError(Exception):\n",
    "    def __init__(self, code, message):\n",
    "        self.code = code\n",
    "        self.message = message\n",
    "        super().__init__(f\"[{code}] {message}\")\n",
    "\n",
    "raise TransactionError(403, \"Unauthorized action\")"
   ]
