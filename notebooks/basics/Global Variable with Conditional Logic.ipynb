   "source": [
    "mode = \"light\"\n",
    "\n",
    "def toggle_mode():\n",
    "    global mode\n",
    "    if mode == \"light\":\n",
    "        mode = \"dark\"\n",
    "    else:\n",
    "        mode = \"light\"\n",
    "\n",
    "toggle_mode()\n",
    "print(mode)  # Output: dark"
   ]
