    "class Balance:\n",
    "    def __init__(self, amount):\n",
    "        self.amount = amount\n",
    "\n",
    "    def __sub__(self, other):\n",
    "        return Balance(self.amount - other.amount)\n",
    "\n",
    "b1 = Balance(100)\n",
    "b2 = Balance(40)\n",
    "\n",
    "result = b1 - b2\n",
    "print(result.amount)  # Output: 60"
   ]
