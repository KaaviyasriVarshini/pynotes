   ],
   "source": [
    "def bold(func):\n",
    "    def wrapper():\n",
    "        print(\"<b>\")\n",
    "        func()\n",
    "        print(\"</b>\")\n",
    "    return wrapper\n",
    "\n",
    "def italic(func):\n",
    "    def wrapper():\n",
    "        print(\"<i>\")\n",
    "        func()\n",
    "        print(\"</i>\")\n",
    "    return wrapper\n",
    "\n",
    "@bold\n",
    "@italic\n",
    "def display():\n",
    "    print(\"Text\")\n",
    "\n",
    "display()"
   ]
