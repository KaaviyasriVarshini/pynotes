    "def threshold_checker(threshold):\n",
    "    def check(value):\n",
    "        return value > threshold\n",
    "    return check\n",
    "\n",
    "high_temp = threshold_checker(40)\n",
    "\n",
    "print(high_temp(35))  # False\n",
    "print(high_temp(45))  # True"
   ]
